<% if deal['guestRating'] == 5 %>
	<%= image_tag("rating5_0.png") %>
<% elsif deal['guestRating'] >= 4.5 %>
	<%= image_tag("rating4_5.png") %>
<% elsif deal['guestRating'] >= 4.0 %>
	<%= image_tag("rating4_0.png") %>
<% elsif deal['guestRating'] >= 3.5 %>
	<%= image_tag("rating3_5.png") %>
<% elsif deal['guestRating'] >= 3.0 %>
	<%= image_tag("rating3_0.png") %>
<% elsif deal['guestRating'] >= 2.5 %>
	<%= image_tag("rating2_5.png") %>
<% elsif deal['guestRating'] >= 2.0 %>
	<%= image_tag("rating2_0.png") %>
<% elsif deal['guestRating'] >= 1.5 %>
	<%= image_tag("rating1_5.png") %>
<% elsif deal['guestRating'] >= 1.0 %>
	<%= image_tag("rating1_0.png") %>
<% elsif deal['guestRating'] >= 0.5 %>
	<%= image_tag("rating0_5.png") %>
<% else %>
	<%= image_tag("rating0_0.png") %>
<% end %>
